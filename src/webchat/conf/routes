# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET / controllers.Application.index()

#Login
GET /login controllers.LoginController.login()
GET /logout controllers.LoginController.logout()
POST /login controllers.LoginController.submit()

#Profile
GET /profile controllers.ProfileController.edit()
POST /profile controllers.ProfileController.submit()


#TestData

GET /Testdata controllers.Application.filltestdata()

# WebSocket
GET /WebSocket controllers.WebSocketTestController.index()

GET /websocket controllers.Application.websocket()

# Json
GET /Json/Channels controllers.JsonTestController.genAuth()
GET /Json/Message controllers.JsonTestController.genMessage()

# Map static resources from the /public folder to the /assets URL path
GET /assets/*file controllers.Assets.at(path="/public", file)
